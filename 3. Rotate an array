#include <stdio.h>

int main() {
    int n, d;
    printf("Enter size of array: ");
    scanf("%d", &n);

    int arr[n];
    printf("Enter %d elements: ", n);
    for (int i = 0; i < n; i++)
        scanf("%d", &arr[i]);

    printf("Enter number of rotations: ");
    scanf("%d", &d);

    d = d % n; // in case d > n

    printf("Array after %d rotations: ", d);
    for (int i = 0; i < n; i++)
        printf("%d ", arr[(i + d) % n]);

    printf("\n");
    return 0;
}
